{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "Compressor",
      "properties": [
        {
          "widget-type": "keyvalue-dropdown",
          "label": "Compress Fields",
          "name": "compressor",
          "description": "List of fields to compress. The key specifies the name of the field to be compressed, with its corresponding value specifying the type of compression that will be applied to it.",
          "widget-attributes": {
            "showDelimiter": "false",
            "dropdownOptions": [
              "SNAPPY",
              "ZIP",
              "GZIP",
              "NONE"
            ],
            "key-placeholder": "Field Name"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "label": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "bytes"
        ],
        "schema-default-type": "bytes"
      }
    }
  ]
}
